extends layout.pug

block content
  ul
    li: a(href='/') root
    li: a(href='/create') create
    li: a(href='/create-interesting') create interesting
  table
    h2 Existing Users
      tr
        th id
        th First Name
        th Last Name
        th Email
        th Age
        th Favorite Beatle
        th Likes Ice Cream
    tbody
      each user in users
        tr
          td= user.id
          td= user.firstName
          td= user.lastName
          td= user.email
          td
          td
          td

